{
    "manifest_version": 2,
    "name": "KOS",
    "author": "Lean",
    "version": "1.0.0",
    "description": "Keyword operation summary",
    "homepage_url": "http://www.baidu.com",
    "icons": {
        "16": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
    },
    "browser_action": {
		"default_icon": "images/icon.png",
		"default_title": "Keyword operation summary",
		"default_popup": "html/popup.html"
	},
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+L",
                "mac": "MacCtrl+Shift+L"
            },
            "description": "Open popup.html"
        }
    }
}
